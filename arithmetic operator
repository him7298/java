public class MyClass {
    public static void main(String args[]) {
      float n1 = 20;
      float n2 = 8;
      float n = n1 + n2;
      
      System.out.println("Sum of n1 + n2 = " + n);
      float m= n1*n2;
       System.out.println("Sum of n1 * n2 = " + m);
       float d= n1/n2;
        System.out.println("Sum of n1 / n2 = " + d);
        float x = n1%n2;
         System.out.println("Sum of n1% n2 = " + x);
    }
}

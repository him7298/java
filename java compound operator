public class MyClass {
    public static void main(String[] args){
        int a =5;
        
        System.out.println("the value of a:" +(a));
          a+=4;
        System.out.println("the value  of a:" +(a));
        a*=4;
         System.out.println("the value  of a:" +(a));
        a-=4;
          System.out.println("the value  of a:" +(a));
          a%=4;
            System.out.println("the value  of a:" +(a));
    }
}

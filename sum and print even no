import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
      int n;
      //int sum = 0;
      Scanner r = new Scanner(System.in);
     n= r.nextInt();
      for(int i=0 ; i <= n; i=i+2  )
      {
          
      
          System.out.print(i + " ");
      }

    
}}
//sum of all even no
import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
      int n;
      int sum = 0;
      Scanner r = new Scanner(System.in);
     n= r.nextInt();
      for(int i=0 ; i <= n; i=i+2  )
      {
          sum=sum+i;
      }
      
          System.out.print(sum);
      

    
}}

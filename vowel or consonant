import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
      Character ch;
      Scanner r= new Scanner(System.in);
      ch=r.next().charAt(0) ;

      
      if(ch=='a'|| ch=='e' || ch=='i' || ch=='o' || ch=='u'  || ch=='s')
      {
             System.out.println("vowels " );
      }
      else
      {
             System.out.println("constant");
      }
    }
}

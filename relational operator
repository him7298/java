public class MyClass {
    public static void main(String args[]) {
      int x=10;
      int y=25;
    

      System.out.println("x,y are :"+ (x  ==  y ));
       System.out.println("x,y are :"+ (x  !=  y ));
        System.out.println("x,y are :"+ (x  >  y ));
         System.out.println("x,y are :"+ (x <  y ));
          System.out.println("x,y are :"+ (x <= y ));
           System.out.println("x,y are :"+ (x  >=  y ));
    }
}
